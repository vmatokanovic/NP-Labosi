int main(void)
{
	float fVal = 5.0;
	double dVal;
	int iVal = -7;
	unsigned long ulVal = 1000;
	dVal = iVal * ulVal;
	printf("iVal * ulVal = %lf\n", dVal);
	dVal = ulVal + fVal;
	printf("ulVal + fVal = %lf\n", dVal);

}

